<div class="panel panel-<%= style %>">
  <div class="panel-heading">
    <h2>New <%= molecules.model_name.human.pluralize.titleize %></h2>
  </div>
  <div class="panel-body">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Name</th>
          <th>Link</th>
        </tr>
      </thead>
      <tbody>
        <% molecules.each do |molecule| %>
        <tr>
          <td><%= molecule.display_name %> </td>
          <td><%= link_to (polymorphic_url(molecule)), (polymorphic_url(molecule)) %> </td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
