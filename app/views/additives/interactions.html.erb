<% if @additive %>
<h1>Interactions of <%= @additive.display_name %></h1>
<div class="">
  <%#= render 'interactions_new' %>
  <%= render "interactions/newinteraction", interactions:@additive.interactions, table_id:'molecules' %>
<%#= render 'forminteractions' %>
</div>
<% end %>
<script type="text/javascript">

  var dataTables = {
    "molecules":NaN
  }

  $(document).on('turbolinks:load',function(){
      dataTables.molecules = $('#int-index-molecules').DataTable(dataTablesPropDefault);
  });

  $(document).on('page.dt', function(){
         hidePopovers();
       });

</script>
